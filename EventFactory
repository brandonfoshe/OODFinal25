public class EventFactory {

    public Event createWorkEvent(int time, int duration, String title, String description, String location){
        //Create Work event
        return new WorkEvent(time, duration, title, description, location);
    }
    
    public Event createPersonalEvent(int time, int duration, String title, String description, String location){
        //Create Personal event
        return new PersonalEvent(time, duration, title, description, location);
    }

    public void clearEvent(Event event) {
        //Get time from event, if it is there, clear that space on the calendar for that duration
        int timeToClear = event.getTime();
    }

}
